{
  "name": "farrow-schema",
  "version": "1.10.0",
  "description": "A powerful and extensible schema builder",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js"
    },
    "./validator": {
      "import": "./esm/validator.js",
      "require": "./dist/validator.js"
    },
    "./formatter": {
      "import": "./esm/formatter.js",
      "require": "./dist/formatter.js"
    }
  },
  "files": [
    "src",
    "dist",
    "esm",
    "validator.d.ts",
    "validator.js",
    "transformer.js",
    "transformer.d.ts",
    "formatter.d.ts",
    "formatter.js"
  ],
  "scripts": {
    "start": ""
  },
  "keywords": [
    "Web Framework",
    "TypeScript",
    "Functional Programming"
  ],
  "author": "Jade Gu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lucifier129/farrow.git"
  },
  "dependencies": {
    "tslib": "^2.1.0"
  }
}
